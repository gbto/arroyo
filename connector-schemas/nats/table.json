{
    "type": "object",
    "title": "NATSTable",
    "properties": {
        "server": {
            "title": "Server",
            "type": "string",
            "description": "The NATS server that should receive the message",
            "examples": [
                "nats://localhost:4222"
            ],
            "format": "uri"
        },
        "subject": {
            "title": "Subject",
            "type": "string",
            "description": "The subject of the NATS message",
            "examples": [
                "my-subject"
            ]
        },
        "token": {
            "title": "Token",
            "type": "string",
            "description": "Optional, the token for NATS server authentication",
            "examples": [
                "my-auth-token"
            ]
        }
    },
    "required": [
        "server",
        "subject"
    ]
}